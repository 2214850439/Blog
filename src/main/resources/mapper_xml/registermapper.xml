<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.svjk.blog.mapper.RegisterMapper">

    <insert id="register" parameterType="com.svjk.blog.pojo.user_info">
        insert into user_info(name,login,password,email,tel,create_date)
        values(#{name},#{login},#{password},#{email},#{tel},#{create_date})
    </insert>
</mapper>